  "scripts": {
    "codegen": "yarn generate",
    "generate": "yarn generate-types && yarn mst-gql:generate",
    "generate-types": "typechain --target=ethers-v5 'abi/*.json' --out-dir generated/typechain",
    "dev": "npx next",
    "build": "next build",
    "export": "next-sitemap && next export && rm public/robots.txt && rm public/sitemap*.xml && echo \"$(date +%s000\n)\" > out/BUILD_TIME.json",
    "test": "jest -w 1",
    "test:ci": "yarn generate && yarn test",
    "start": "next start",
    "pre-commit": "lint-staged",
    "lint": "eslint . --ext .tsx,.ts",
    "lint:fix": "eslint . --ext .tsx,.ts --fix",
    "mst-gql:generate": "./internals/mst-gql-generate.bash",
    "certify-fe": "node internals/certify-fe.mjs",
    "decode-build-certification": "node internals/decode-build-certification.mjs",
    "local-devnet": "node -r esbuild-register internals/local-devnet.ts"
  },
